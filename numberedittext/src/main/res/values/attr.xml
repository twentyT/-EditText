<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="EditTextCount">
        <attr name="indexColor" format="color"></attr>
        <attr name="indexSize" format="integer"></attr>
        <attr name="indexMaxNumber" format="integer"></attr>
        <attr name="indexMarginBottom" format="integer"></attr>
        <attr name="indexMarginRight" format="integer"></attr>
        <attr name="defaultHeight" format="integer"></attr>
        <attr name="textType">
            <enum name="defalut" value="0"></enum>
            <enum name="defalut_bold" value="1"></enum>
            <enum name="monospace" value="2"></enum>
            <enum name="sans_serif" value="3"></enum>
            <enum name="serif" value="4"></enum>
        </attr>
    </declare-styleable>
</resources>